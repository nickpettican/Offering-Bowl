{
    "name": "offering-bowl-server",
    "version": "0.0.1",
    "private": true,
    "scripts": {
        "build": "npm run clean && tsc --build tsconfig.json && cp .env dist/ || true",
        "build:prod": "npm run clean && tsc --build tsconfig.prod.json",
        "clean": "rm -rf dist",
        "check-types": "tsc -p . --noEmit",
        "check-types:watch": "tsc -w",
        "eslint": "eslint src",
        "eslint-fix": "eslint src --fix",
        "prettier": "prettier --write .",
        "start": "ts-node-dev --inspect --respawn --transpile-only --watch ./src ./src/bin/www.ts",
        "start:prod": "node dist/src/bin/www.js",
        "test": "jest --config jest.config.js",
        "test:coverage": "jest --coverage",
        "test:watch": "jest --watch",
        "qc": "npm test && npm run eslint && npm run check-types && npm run prettier"
    },
    "dependencies": {
        "@aws-sdk/client-dynamodb": "3.687.0",
        "@aws-sdk/lib-dynamodb": "3.689.0",
        "@types/cookie-parser": "1.4.7",
        "@types/debug": "4.1.12",
        "@types/morgan": "1.9.9",
        "cookie-parser": "1.4.7",
        "cors": "2.8.5",
        "debug": "4.3.7",
        "express": "4.21.1",
        "express-rate-limit": "7.4.1",
        "firebase-admin": "13.0.0",
        "helmet": "8.0.0",
        "morgan": "1.10.0",
        "ts-node": "10.9.2",
        "winston": "3.17.0"
    },
    "devDependencies": {
        "@eslint/js": "^9.14.0",
        "@types/cors": "^2.8.17",
        "@types/express": "5.0.0",
        "@types/jest": "^29.5.14",
        "@types/node": "22.9.0",
        "@types/supertest": "^6.0.2",
        "@typescript-eslint/eslint-plugin": "^8.14.0",
        "@typescript-eslint/parser": "^8.14.0",
        "dotenv": "16.4.5",
        "eslint": "^9.14.0",
        "eslint-config-prettier": "^9.1.0",
        "eslint-plugin-import": "^2.31.0",
        "eslint-plugin-node": "^11.1.0",
        "eslint-plugin-prettier": "^5.2.1",
        "globals": "^15.12.0",
        "jest": "^29.7.0",
        "prettier": "^3.3.3",
        "supertest": "^7.0.0",
        "ts-jest": "^29.2.5",
        "ts-node-dev": "^2.0.0",
        "typescript": "5.6.3",
        "typescript-eslint": "^8.14.0"
    }
}
